{
  "name": "memebot",
  "version": "0.1.0",
  "description": "Memebot",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "migrate": "pnpm build:db && node --enable-source-maps dist/migrator.js",
    "dev": "pnpm build && node --enable-source-maps dist/index.js",
    "build": "esbuild src/index.ts --outdir=dist --format=esm --bundle --platform=node --sourcemap=inline --external:'./node_modules/*'",
    "build:db": "esbuild src/db/migrator.ts --outdir=dist --format=esm --bundle --platform=node --sourcemap=inline --external:'./node_modules/*' && esbuild src/db/migrations/*.ts --outdir=dist/migrations --format=esm --bundle --platform=node --sourcemap=inline --external:'./node_modules/*'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Arjun Chib",
  "license": "ISC",
  "dependencies": {
    "@discordjs/opus": "^0.7.0",
    "@discordjs/voice": "^0.8.0",
    "@ffmpeg/core": "^0.10.0",
    "@ffmpeg/ffmpeg": "^0.10.1",
    "discord.js": "^13.6.0",
    "esbuild": "^0.14.18",
    "prism-media": "^1.3.2",
    "sequelize": "^6.16.0",
    "sodium": "^3.0.2",
    "sqlite3": "^5.0.2",
    "umzug": "^3.0.0",
    "ytdl-core": "^4.10.1"
  }
}
